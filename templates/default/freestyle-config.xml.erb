<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description><%= @description %></description>
<% unless @display_name.nil? -%>
  <displayName><%= @display_name %></displayName>
<% end -%>
  <keepDependencies>false</keepDependencies>
  <properties>
<% @properties.each do |property, options| %>
<%= partial_template(property, 'property', options, @partial_template_cookbook) %>
<% end %>
  </properties>
<% if @scm.empty? -%>
  <scm class="hudson.scm.NullSCM"/>
<% else -%>
<%= partial_template(@scm.keys[0], 'scm', @scm.values[0], @partial_template_cookbook) %>
<% end %>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
<% @triggers.each do |trigger, options| %>
<%= partial_template(trigger, 'trigger', options, @partial_template_cookbook) %>
<% end %>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
<% @builders.each do |builder, options| %>
<%= partial_template(builder, 'builder', options, @partial_template_cookbook) %>
<% end %>
  </builders>
  <publishers>
<% @publishers.each do |publisher, options| %>
<%= partial_template(publisher, 'publisher', options, @partial_template_cookbook) %>
<% end %>
  </publishers>
  <buildWrappers>
<% @wrappers.each do |wrapper, options| %>
<%= partial_template(wrapper, 'wrapper', options, @partial_template_cookbook) %>
<% end %>
  </buildWrappers>
</project>
